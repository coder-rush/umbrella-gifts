	    function check_login_stat(thing){
	        return $.ajax({
	            url: 'http://localhost/ecomx/index.php/home/check_login/'+thing
	        });
	    }
		
		function load_iamges(){
			$('body').find('.image_delay').each(function(){
				var src = $(this).data('src');
				if($(this).is('img')){
					$(this).attr('src',src);			
				} else {
					$(this).css('background-image',"url('"+src+"')");			
				}
			});	
		}

